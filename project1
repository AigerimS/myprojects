  width: 160px;
  display: block;
  color: black;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  font-family: 'Playfair Display', serif;
  font-size:15px;
}

li a:hover {
  background-color: rgb(240, 123, 240); 
  
  transition: 0.5s;
  text-transform: uppercase;

}


